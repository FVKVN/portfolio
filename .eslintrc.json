{
  "root": true,
  "env": {
    "browser": true,
    "jquery": true
  },
  "globals": {
    "$": true,
    "jQuery": true,
    "window": true,
    "dataLayer": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": ["error", 4],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "no-unused-vars": [1, {"vars": "local", "args": "none"}]
  }
}